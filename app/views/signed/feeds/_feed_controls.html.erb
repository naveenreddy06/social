<ul class="cssMenu">
   <li><a style="float:right"><img src="/img/relayimg/report_abuse.png"/></a>
     <ul>
       <% if feed.user == current_user %>
         <li style="font-size:13px;font-style:italic;">
         <a onclick="deletePost('<%= feed.id %>');" id="<%= feed.id %>" >Delete Post</a></li>
       <% else %>
        <li style="font-size:13px;font-style:italic">
        <a onclick="hidePost('<%= feed.id %>');" id="<%= feed.id %>">Hide Post</a></li>
      <% end %>
     </ul>
   </li>
</ul>