$('#myemotionsModal').modal('hide');
<% if @feed %>
<% current_action =  params[:update_tag].keys.first %>
 $("#<%= @feed.id.to_s %>_<%= current_action %>").replaceWith('<%= escape_javascript(send("#{current_action}_status", @feed)) %>');
if(pageIn == "Favorites") {
  $("#tdFeed_<%= @feed.id %>").hide();
}
<% else %>
  bootbox.alert("Post has been deleted at the source.", function() {
    $(".modal-footer").trigger("click");
    $("#tdFeed_<%= params[:feed_id] %>").hide();
  });
<% end %>
