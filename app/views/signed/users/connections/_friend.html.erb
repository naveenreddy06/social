<% friendrequest = @friendrequest || UserFriend.where(:user_id => current_user.id , :friend_id => friend.id).first %>

  <td>
    <div class="span6">
      <%= image_tag friend.profile_image_thumb %>
    </div>  
    <div class="span6">
      <span class="span_content" >
        <a id="<%= friend.id %>" href="#myModal" data-toggle="modal" onclick="currentSelect = $(this).attr('id');">
          <div style="color:green">
            <%= friend.first_name.capitalize + " " + friend.last_name.capitalize %>
          </div>
        </a>
        <%= friendrequest.connection.capitalize %>
      </span>
    </div>
  </td>
  <td>
    <% if friendrequest.block %>
      <%=link_to "UnBlock", unblock_signed_users_path(:id => friend.id, :partial_name => "lists_update"),:remote => true, :class => "common_link_font"  %>
    <% else %>
      <%=link_to "Block", block_signed_users_path(:id => friend.id, :partial_name => "lists_update"),:remote => true, :class => "common_link_font" %>
    <% end %>
  </td>





